<Window x:Class="ControlDeProcesos.TablaProcesos"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControlDeProcesos"
        mc:Ignorable="d"
        Title="Tabla de procesos" Height="300" Width="300">
    <Grid>
        <DataGrid x:Name="dgProcesos" ItemsSource="{Binding TablaProcesos, Mode=OneWay}" Margin="10"/>
        <!--<DataGrid x:Name="dgProcesos"  Margin="10"/>-->

    </Grid>
</Window>
